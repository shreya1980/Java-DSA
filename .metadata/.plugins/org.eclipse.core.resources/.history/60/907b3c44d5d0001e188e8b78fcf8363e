package com.shreya;
import java.util.*;

public class Factorial {
	public static int findFactorial(int a) {
		for(a=n;a>=1;a--) {
			a = a*n;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n= sc.nextInt();
		

	}

}
