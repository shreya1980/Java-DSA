package com.shreya;
import java.util.*;

public class Factorial {
	public static int findFactorial(int n) {
		int factorial;
		for(int i=n;i>=1;i--) {
			factorial=factorial*i;
		}
		return factorial;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n= sc.nextInt();
		

	}

}
